        <attr name="finalHeight" format="dimension" />
    </declare-styleable>


    <declare-styleable name="CustomView1">
        <attr name="minHeight" format="dimension|reference" />
        <attr name="minWidth" format="dimension|reference" />
        <attr name="backColor" format="color|reference" />
