<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android = "http://schemas.android.com/apk/res/android"
    package = "com.xiao.demo">

    <uses-permission android:name = "android.permission.INTERNET" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name = "android.permission.GET_ACCOUNTS" />
    <uses-permission android:name = "android.permission.READ_PROFILE" />
    <uses-permission android:name = "android.permission.READ_CONTACTS" />
    <uses-permission android:name = "android.permission.READ_PHONE_STATE" />

    <application
        android:name = ".MyApplication"
        android:allowBackup = "true"
        android:icon = "@mipmap/ic_launcher"
        android:label = "@string/app_name"
        android:roundIcon = "@mipmap/ic_launcher_round"
        android:supportsRtl = "true"
        android:theme = "@style/AppTheme">

        <!-- <activity android:name=".kotlin.KotlinMainActivity"> -->
        <!-- <intent-filter> -->
        <!-- <action android:name="android.intent.action.MAIN" /> -->

        <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
        <!-- </intent-filter> -->
        <!-- </activity> -->
        <activity
            android:name = ".MainActivity"
            android:screenOrientation = "portrait">
            <intent-filter>
                <action android:name = "android.intent.action.MAIN" />

                <category android:name = "android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name = "android.intent.action.VIEW" />

                <category android:name = "android.intent.category.DEFAULT" />
                <category android:name = "android.intent.category.BROWSABLE" />

                <data
                    android:host = "xiao.demo.com"
                    android:scheme = "http" />
            </intent-filter>
        </activity>
        <activity
            android:name = ".kotlin.KotlinMainActivity"
            android:theme = "@style/AppTheme.NoActionBar" />
        <activity
            android:name = ".WebViewActivity"
            android:theme = "@style/AppTheme.NoActionBar" />
        <activity
            android:name = ".ImageLoader.ImageLoaderActivity"
            android:screenOrientation = "portrait" />
        <activity
            android:name = ".arouter.ArouterActivity"
            android:screenOrientation = "portrait" />
        <activity
            android:name = ".materialdesign.WidgetActivity"
            android:theme = "@style/AppTheme.NoActionBar" />
        <activity
            android:name = ".materialdesign.ActionBarActivity"
            android:theme = "@style/AppTheme.NoActionBar" />
        <activity android:name = ".materialdesign.NormalActionBarActivity" />
        <activity android:name = ".materialdesign.CustomViewActionBarActivity" />
        <activity
            android:name = ".materialdesign.ToolbarActionBarActivity"
            android:theme = "@style/AppTheme.NoActionBar" />

        <!-- CoordinatorLayout -->
        <activity
            android:name = ".materialdesign.CoordinatorLayoutActivity"
            android:theme = "@style/AppTheme.NoActionBar" />
        <activity
            android:name = ".materialdesign.SnackbarFABActivity"
            android:theme = "@style/AppTheme.NoActionBar" />
        <activity
            android:name = ".materialdesign.ScrollingActivity"
            android:label = "@string/title_activity_scrolling"
            android:theme = "@style/AppTheme.NoActionBar" />
        <activity
            android:name = ".materialdesign.EnterAlwaysActivity"
            android:label = "Expanding and Collapsing Toolbars"
            android:screenOrientation = "portrait"
            android:theme = "@style/AppTheme.NoActionBar" />
        <activity
            android:name = ".materialdesign.EnteralwaysCollapsedActivity"
            android:theme = "@style/AppTheme.NoActionBar" />
        <activity
            android:name = ".materialdesign.ExitUtilCollapsedActivity"
            android:theme = "@style/AppTheme.NoActionBar" />
        <activity
            android:name = ".materialdesign.SnapActivity"
            android:theme = "@style/AppTheme.NoActionBar" />
        <activity
            android:name = ".materialdesign.CollapsedEffectsActivity"
            android:theme = "@style/AppTheme.CollapsedEffects" />
        <activity
            android:name = ".materialdesign.ParallaxAnimationActivity"
            android:theme = "@style/AppTheme.CollapsedEffects" />
        <activity
            android:name = ".materialdesign.BottomSheetNestedScrollViewActivity"
            android:theme = "@style/AppTheme.NoActionBar" />
        <activity
            android:name = ".materialdesign.BottomSheet2Activity"
            android:theme = "@style/AppTheme.NoActionBar" />
        <activity
            android:name = ".materialdesign.ModalSheetsActivity"
            android:theme = "@style/AppTheme.NoActionBar" />

        <activity android:name = ".materialdesign.MaterialDesignActivity" />

        <activity android:name = ".materialdesign.ViewPagerAcivity" />
        <!-- CoordinatorLayout Behavior -->

        <activity
            android:name = ".materialdesign.BehaviorActivity"
            android:theme = "@style/AppTheme.NoActionBar" />
        <activity
            android:name = ".materialdesign.SimpleBehaviorActivity"
            android:theme = "@style/AppTheme.NoActionBar" />
        <activity
            android:name = ".materialdesign.UCBehaviorActivity"
            android:theme = "@style/AppTheme.NoActionBar" />
        <activity
            android:name = ".materialdesign.CustomFABBehaviorActivity"
            android:theme = "@style/AppTheme.CollapsedEffects" />

        <!-- custom  view -->
        <activity
            android:name = ".customview.CustomViewActivity"
            android:theme = "@style/AppTheme.NoActionBar" />
        <activity
            android:name = ".system.HandlerThreadActivity"
            android:label = "@string/title_activity_handler_thread"
            android:theme = "@style/AppTheme.NoActionBar" />
        <activity
            android:name = ".system.DownLoadManagerActivity"
            android:label = "@string/title_activity_down_load_manager"
            android:theme = "@style/AppTheme.NoActionBar" />

        <activity
            android:name = ".animation.AnimationManagerActivity"
            android:launchMode = "singleTop"
            android:screenOrientation = "portrait" />

        <activity
            android:name = ".animation.AnimationActivity"
            android:launchMode = "singleTop"
            android:screenOrientation = "portrait" />

        <activity
            android:name = ".animation.InterpolaterActivity"
            android:launchMode = "singleTop"
            android:screenOrientation = "portrait" />
        <activity
            android:name = ".animation.ObjectAnimationActivity"
            android:launchMode = "singleTop"
            android:screenOrientation = "portrait" />
        <activity
            android:name = ".animation.ValueAnimatorActivity"
            android:launchMode = "singleTop"
            android:screenOrientation = "portrait" />

    </application>

</manifest>